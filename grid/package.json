{
  "name": "rpg-character-manager",
  "version": "1.0.0",
  "description": "Rpg Character Manager",
  "config": {
    "csp": {
      "dev": "script-src * 'unsafe-inline'; style-src * 'unsafe-inline'",
      "prod": "script-src 'self'; style-src 'self' 'unsafe-inline'; connect-src 'self' https://login.microsoftonline.com"
    },
    "alias": {
      "react-intl$": "react-intl/dist/react-intl.js"
    },
    "buildDir": "./build",
    "devHost": "localhost",
    "devPort": 8000
  },
  "scripts": {
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "start": "webpack-dev-server",
    "preprebuild": "npm run clean && npm run test",
    "build": "webpack --colors --progress",
    "prepretest": "npm run test-typecheck && npm run test-audit && npm run test-lint",
    "test": "karma start",
    "test-audit": "nsp check",
    "test-continuous": "karma start --auto-watch --no-single-run",
    "test-lint": "eslint ./src --ext .js --ext .jsx",
    "test-typecheck": "flow check"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-plugin-typecheck": "^3.6.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-runtime": "^6.2.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-flow-vars": "^0.1.3",
    "eslint-plugin-react": "^3.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flow-bin": "^0.21.0",
    "flowcheck": "^0.2.7",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.7.2",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-jsdom-launcher": "^3.0.0",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "nsp": "^2.2.0",
    "opn": "^4.0.0",
    "postcss-loader": "^0.8.0",
    "react-addons-test-utils": "^0.14.6",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "toolbox-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.7.2",
    "webpack-notifier": "^1.2.1"
  },
  "dependencies": {
    "airflux": "^0.5.1",
    "babel-polyfill": "^6.2.0",
    "classnames": "^2.2.1",
    "data-formats": "^0.1.1",
    "font-awesome": "^4.5.0",
    "history": "^2.0.0-rc2",
    "immutable": "^3.7.6",
    "lodash": "^4.1.0",
    "moment": "^2.11.0",
    "normalize.css": "^3.0.3",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.5",
    "react-intl": "^2.0.0-beta-2",
    "react-motion": "^0.4.2",
    "react-router": "^2.0.0-rc4",
    "react-select": "^1.0.0-beta8",
    "react-toolbox": "^0.14.1",
    "whatwg-fetch": "^0.11.0"
  }
}
